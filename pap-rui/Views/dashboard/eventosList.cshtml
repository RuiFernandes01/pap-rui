@model List<eventos>

@foreach (eventos item in Model)
{
    <li class="d-flex">
        <div>@item.titulo</div>
        <div>@item.subtitulo</div>
        <div>@item.imagem</div>
        <div>@item.datainicio</div>
        <div>@item.datafim</div>
        <div>@item.descricao</div>
        <div>@item.latlang</div>
        <a title="Apagar" class="delete-btn" data-id="@item.id"><i class="fas fa-trash-alt"></i></a>
    </li>
}
